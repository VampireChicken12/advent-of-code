{
	"name": "adventofcode-generator",
	"version": "1.0.0",
	"description": "Command line utility for generating Advent of Code folder and files",
	"main": "./dist/index.js",
	"type": "commonjs",
	"scripts": {
		"start": "node ./dist/index.js",
		"dev": "ts-node-dev --transpile-only --poll --respawn --ignore-watch node_modules src/index.ts",
		"dev_inspect": "ts-node-dev --transpile-only --inspect --poll --respawn --ignore-watch node_modules src/index.ts",
		"dev_nodemon": "nodemon --watch './src/**/*.ts' --exec ts-node --transpile-only ./src/index.ts",
		"build": "tsc"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"axios": "^1.2.1",
		"cheerio": "^1.0.0-rc.12",
		"dotenv": "^16.0.3",
		"fs": "^0.0.1-security",
		"inquirer": "^8.2.4",
		"path": "^0.12.7"
	},
	"devDependencies": {
		"@swc/core": "^1.3.23",
		"@types/inquirer": "^9.0.3",
		"@types/node": "^18.11.15",
		"nodemon": "^2.0.20",
		"swc": "^1.0.11",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"typescript": "^4.9.4"
	}
}
